{
  "name": "api-himno",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start:old": "node --watch --loader ts-node/esm ./src/app.ts",
    "dev:old": "node --watch --loader ts-node/esm ./src/app.ts",
    "start:dev": "node --watch --loader ts-node/esm ./src/main.ts",
    "build:old": "tsc",
    "start": "tsx src/main.ts",
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "serve": "node dist/main.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/uuid": "^9.0.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
